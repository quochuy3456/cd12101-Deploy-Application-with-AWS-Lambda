{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "update_todo",
    "type": "object",
    "properties": {
        "name": {
            "type":"string",
            "pattern":"^.*[a-zA-Z0-9]+.*$",
            "minLength": 1
        },
        "dueDate": {
            "type": "string"
        },
        "done": {
            "type": "boolean"
        }
    },
    "required":[
        "name",
        "dueDate",
        "done"
    ]
}